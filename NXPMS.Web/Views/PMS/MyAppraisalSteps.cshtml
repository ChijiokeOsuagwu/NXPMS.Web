
@model NXPMS.Web.Models.PMSViewModels.MyAppraisalStepsViewModel
<div class="container">
    <h4 class="mb-1">@Model.ReviewSessionName for @Model.AppraiseeName</h4>
    <hr />
    <div class="row">
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <div class="container-fluid">
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNavDropdown" aria-controls="navbarNavDropdown" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarNavDropdown">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a asp-controller="PMS" asp-action="MyAppraisalSessions" title="Go Back" class="btn btn-sm btn-outline-light text-dark">
                                <i class="bi bi-backspace fw-bolder" style="font-size: 1.0rem;"></i>&nbsp;<span class="fw-bold ml-1" style="font-size: 0.9rem;">Back</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a asp-controller="PMS" asp-action="ReviewSessionInfo" asp-route-id="@Model.ReviewSessionId" title="Show Information" class="btn btn-sm btn-light text-dark">
                                <i class="bi bi-info-lg fw-bolder" style="font-size: 1.0rem;"></i><span class="fw-bold ml-1" style="font-size: 0.9rem;">Details</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a asp-Controller="PMS" asp-action="AppraisalGradesInfo" asp-route-id="@Model.ReviewSessionId" title="Change Image" class="btn btn-sm btn-light text-dark">
                                <i class="bi bi-percent fw-bolder" style="font-size: 1.0rem;"></i><span class="fw-bold ml-1" style="font-size: 0.9rem;">Appraisal Grades</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a asp-controller="PMS" asp-action="AppraisalSessionSchedules" asp-route-id="@Model.ReviewSessionId" title="Appraisal Schedules" class="btn btn-sm btn-light text-dark">
                                <i class="bi bi-calendar4-week fw-bolder" style="font-size: 1.0rem"></i><span class="fw-bold ml-1" style="font-size: 0.9rem;">Appraisal Schedules</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a asp-controller="PMS" asp-action="MoveToPreviousStep" asp-route-id="@Model.ReviewHeaderId" title="Return to Previous Step" class="btn btn-sm btn-light text-dark">
                                <i class="bi bi-arrow-90deg-up fw-bolder" style="font-size: 1.0rem"></i><span class="fw-bold ml-1" style="font-size: 0.9rem;">Return To Previous</span>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </div>
    <div class="mb-1">
        <input type="hidden" asp-for="ReviewHeaderId" />
        <div>
            @if (!String.IsNullOrEmpty(Model.ViewModelErrorMessage))
            {
                <div class="alert alert-danger alert-dismissible fade show" role="alert">
                    <i class="bi bi-x-circle"></i>
                    <span class="fw-bold">@Model.ViewModelErrorMessage</span>
                    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                </div>
            }
        </div>
        <div class="row">
            @if (Model == null || Model.AppraisalStageList == null || Model.AppraisalStageList.Count < 1)
